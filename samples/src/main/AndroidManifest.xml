<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.movingbits.datatables.samples" >

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="net.movingbits.datatables.samples.MainActivity"
            android:label="@string/app_name"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.movingbits.datatables.samples.SimpleTable"
            android:label="@string/simple_adapter" >
        </activity>
        <activity
            android:name="net.movingbits.datatables.samples.StyleTable"
            android:label="@string/style_adapter" >
        </activity>
        <activity
            android:name="net.movingbits.datatables.samples.FamilyTable"
            android:label="@string/family_adapter" >
        </activity>
    </application>

</manifest>
